{#
/**
 * @file
 * Theme override to display an input form element of type="submit".
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 * @see uikit_admin_preprocess_input()
 *
 * @ingroup uikit_admin_themeable
 */
#}
{%
  set classes = [
    errors ? 'uk-form-danger',
  ]
%}
<button{{ attributes.addClass(classes) }}>{{ value }}</button>{{ children }}
